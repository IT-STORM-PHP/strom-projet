{
    "name": "storm_bin/project",
    "type": "library",
    "description": "Storm v0",
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^10.0",
        "illuminate/view": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/events": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ismael\\Project\\": "framework/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StormBin\\Project\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Storm",
            "email": "storm@mail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
