{
  "name": "storm_bin/project",
  "type": "project",
  "description": "Un squelette de projet Storm v0",
  "require": {
    "php": ">=8.0",
    "storm_bin/storm": "*"
  },
  "autoload": {
    "psr-4": {
      "StormBin\\Package\\": "vendor/storm/framework/src/",
      "App\\":"app",
      "App\\Api\\":"app/api",
      "App\\Web\\":"app/web",
      "App\\Model\\":"app/Model"
