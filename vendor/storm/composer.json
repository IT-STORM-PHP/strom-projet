{
    "name": "storm_bin/package",
    "type": "library",
    "keywords": ["framework", "storm", "laravel"],
    "description": "Ceci est un framework générant du code inspiré de laravel pour aider les developpeur à créer des applications web et qui utilise au paravant Laravel.",
    
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^12.0",
        "illuminate/view": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/events": "^12.0",
        "illuminate/http": "^12.2",
        "illuminate/routing": "^12.2",
        "illuminate/log": "^12.2",
        "illuminate/config": "^12.2",
        "illuminate/pagination": "^12.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "StormBin\\Package\\": "framework/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StormBin\\Package\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Storm",
            "email": "godwilloussou@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/IT-STORM-PHP/strom-projet",
        "source": "https://github.com/IT-STORM-PHP/strom-projet"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
